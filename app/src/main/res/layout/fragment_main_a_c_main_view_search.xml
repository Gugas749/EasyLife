<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/colorPrimary"
    tools:context=".fragments.mainactivityfragments.main_view.search.MainACMainViewSearchFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/imageView_ChangeToDefault_FragMainACMainView"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_gravity="start"
            android:rotation="180"
            android:padding="@dimen/_4sdp"
            android:src="@drawable/arrow_icon"
            app:tint="?attr/textAppearanceBodySmall"/>
        <TextView
            android:id="@+id/textView_spinnerSpendigsAccountsExplain_FragMainACMainViewSearch"
            android:layout_below="@+id/imageView_ChangeToDefault_FragMainACMainView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginLeft="@dimen/_16sdp"
            android:layout_marginRight="@dimen/_16sdp"
            android:text="@string/mainAc_FragMainViewSearch_ExplainatoryTextView_1_Text"
            android:textStyle="bold"
            android:textColor="?attr/textAppearanceBodyMedium"
            android:textAlignment="center"
            android:textSize="@dimen/_18sdp" />
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_spinnerSpendigsAccountsHolder_FragMainACMainViewSearch"
            android:layout_below="@+id/textView_spinnerSpendigsAccountsExplain_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorSecondary"
            app:cardCornerRadius="@dimen/_23sdp">
            <com.skydoves.powerspinner.PowerSpinnerView
                android:id="@+id/spinner_spendigsAccounts_FragMainACMainViewSearch"
                android:hint="@string/alertDialog_LongPressMainViewObjects_spinner_SpendingsAccounts_StarterText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_16sdp"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_gravity="center"
                android:background="?attr/colorSecondary"
                android:gravity="center"
                android:padding="@dimen/_8sdp"
                android:textColor="?attr/textAppearanceBodySmall"
                android:textColorHint="?attr/textAppearanceBodySmall"
                android:textSize="@dimen/_12sdp"
                app:spinner_arrow_gravity="end"
                app:spinner_arrow_padding="@dimen/_6sdp"
                app:spinner_divider_color="?attr/textAppearanceBodySmall"
                app:spinner_divider_show="false"
                app:spinner_divider_size="0.4dp"
                app:spinner_item_height="@dimen/_35sdp"
                app:spinner_popup_animation="dropdown"
                app:spinner_popup_background="@drawable/round_corners_alert_dialog"
                app:spinner_popup_elevation="@dimen/_11sdp" />
        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/textView_spinnerSpendigsSubAccountsExplain_FragMainACMainViewSearch"
            android:layout_below="@+id/cardView_spinnerSpendigsAccountsHolder_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginLeft="@dimen/_16sdp"
            android:layout_marginRight="@dimen/_16sdp"
            android:text="@string/mainAc_FragMainViewSearch_ExplainatoryTextView_2_Text"
            android:textStyle="bold"
            android:textColor="?attr/textAppearanceBodyMedium"
            android:textAlignment="center"
            android:textSize="@dimen/_18sdp" />
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_spinnerSpendigsSubAccountsHolder_FragMainACMainViewSearch"
            android:layout_below="@+id/textView_spinnerSpendigsSubAccountsExplain_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorSecondary"
            app:cardCornerRadius="@dimen/_23sdp">
            <com.skydoves.powerspinner.PowerSpinnerView
                android:id="@+id/spinner_spendigsSubAccounts_FragMainACMainViewSearch"
                android:hint="@string/alertDialog_LongPressMainViewObjects_spinner_SpendingsSubAccounts_StarterText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_16sdp"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_gravity="center"
                android:background="?attr/colorSecondary"
                android:gravity="center"
                android:padding="@dimen/_8sdp"
                android:textColor="?attr/textAppearanceBodySmall"
                android:textColorHint="?attr/textAppearanceBodySmall"
                android:textSize="@dimen/_12sdp"
                app:spinner_arrow_gravity="end"
                app:spinner_arrow_padding="@dimen/_6sdp"
                app:spinner_divider_color="?attr/textAppearanceBodySmall"
                app:spinner_divider_show="false"
                app:spinner_divider_size="0.4dp"
                app:spinner_item_height="@dimen/_35sdp"
                app:spinner_popup_animation="dropdown"
                app:spinner_popup_background="@drawable/round_corners_alert_dialog"
                app:spinner_popup_elevation="@dimen/_11sdp" />
        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/textView_design_1_FragMainACMainViewSearch"
            android:layout_below="@+id/cardView_spinnerSpendigsSubAccountsHolder_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginLeft="@dimen/_16sdp"
            android:layout_marginRight="@dimen/_16sdp"
            android:text="@string/mainAc_FragMainViewSearch_ExplainatoryTextView_3_Text"
            android:textStyle="bold"
            android:textColor="?attr/textAppearanceBodyMedium"
            android:textAlignment="center"
            android:textSize="@dimen/_18sdp"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_textViewDateHolder_FragMainACMainViewSearch"
            android:layout_below="@+id/textView_design_1_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorSecondary"
            app:cardCornerRadius="@dimen/_23sdp">
            <LinearLayout
                android:id="@+id/linearLayout_textViewDateHolder_FragMainACMainViewSearch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView_design_1_FragMainACMainViewSearch">
                <TextView
                    android:id="@+id/textView_date_FragMainACMainViewSearch"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:text="dd-MM-yyyy"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_16sdp"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/button_ChangeHours_FragMainACMainViewSearch"
                    android:layout_marginLeft="@dimen/_6sdp"
                    android:layout_marginRight="@dimen/_12sdp"
                    android:layout_marginTop="@dimen/_6sdp"
                    android:layout_marginBottom="@dimen/_6sdp"
                    android:layout_gravity="center|left"
                    android:textSize="@dimen/_12sdp"
                    android:paddingTop="@dimen/_11sdp"
                    android:paddingBottom="@dimen/_11sdp"
                    android:paddingLeft="@dimen/_20sdp"
                    android:paddingRight="@dimen/_20sdp"
                    android:text="@string/general_change"
                    android:textColor="?attr/textAppearanceBodySmall"
                    android:background="@drawable/round_button_secondary"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>
        <com.github.mikephil.charting.charts.PieChart
            android:id="@+id/pieChart_FragMainACMainViewSearch"
            android:layout_below="@+id/cardView_textViewDateHolder_FragMainACMainViewSearch"
            android:layout_width="@dimen/_130sdp"
            android:layout_height="@dimen/_130sdp"
            android:layout_marginBottom="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_8sdp" />
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_InformationHolder_FragMainACMainViewSearch"
            android:layout_below="@+id/cardView_textViewDateHolder_FragMainACMainViewSearch"
            android:layout_toRightOf="@+id/pieChart_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorSecondary"
            app:cardCornerRadius="@dimen/_23sdp">
            <RelativeLayout
                android:id="@+id/linearLayout_InformationHolder_FragMainACMainViewSearch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_6sdp"
                android:layout_marginRight="@dimen/_12sdp"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:layout_below="@+id/textView_design_1_FragMainACMainViewSearch">
                <TextView
                    android:id="@+id/textView_Name_FragMainACMainViewSearch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/mainAc_FragMainViewSearch_InformationCard_Name_Text"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_14sdp"/>
                <TextView
                    android:id="@+id/textView_PercentageText_FragMainACMainViewSearch"
                    android:layout_below="@+id/textView_Name_FragMainACMainViewSearch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/mainAc_FragMainViewSearch_InformationCard_Percentage_Text"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_14sdp"/>
                <TextView
                    android:id="@+id/textView_Percentage_FragMainACMainViewSearch"
                    android:layout_below="@+id/textView_PercentageText_FragMainACMainViewSearch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="00%"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_14sdp"/>
                <TextView
                    android:id="@+id/textView_TotalSpentText_FragMainACMainViewSearch"
                    android:layout_below="@+id/textView_Percentage_FragMainACMainViewSearch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/mainAc_FragMainViewSearch_InformationCard_TotalSpend_Text"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_14sdp"/>
                <TextView
                    android:id="@+id/textView_TotalSpent_FragMainACMainViewSearch"
                    android:layout_below="@+id/textView_TotalSpentText_FragMainACMainViewSearch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="000,00€"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_14sdp"/>
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <FrameLayout
            android:id="@+id/frameLayout_SearchButtonHolder_FragMainACMainViewSearch"
            android:layout_below="@+id/cardView_InformationHolder_FragMainACMainViewSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_Search_FragMainACMainViewSearch"
                android:layout_marginLeft="@dimen/_6sdp"
                android:layout_marginRight="@dimen/_6sdp"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:textSize="@dimen/_12sdp"
                android:paddingTop="@dimen/_11sdp"
                android:paddingBottom="@dimen/_11sdp"
                android:paddingLeft="@dimen/_20sdp"
                android:paddingRight="@dimen/_20sdp"
                android:text="@string/general_Search"
                android:layout_gravity="center|right"
                android:textColor="?attr/textAppearanceBodySmall"
                android:background="@drawable/round_button_secondary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </FrameLayout>
    </RelativeLayout>
</FrameLayout>