<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.alertDialogFragments.AlertDialogLongPressMainViewObjectsFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/textView_spinnerWidgetStyleExplain_FragAlertDialogLongPressMainViewObjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_weight="1"
            android:text="@string/alertDialog_LongPressMainViewObjects_TextView_Styles_Text"
            android:textAlignment="center"
            android:textColor="?attr/textAppearanceBodySmall"
            android:textSize="@dimen/_18sdp" />
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_spinnerWidgetStyleHolder_FragAlertDialogLongPressMainViewObjects"
            android:layout_below="@+id/textView_spinnerWidgetStyleExplain_FragAlertDialogLongPressMainViewObjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorPrimary"
            app:cardCornerRadius="@dimen/_23sdp">
            <com.skydoves.powerspinner.PowerSpinnerView
                android:id="@+id/spinner_widgetStyle_FragAlertDialogLongPressMainViewObjects"
                android:hint="@string/alertDialog_LongPressMainViewObjects_spinner_WidgetStyle_StarterText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_16sdp"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_gravity="center"
                android:background="?attr/colorPrimary"
                android:gravity="center"
                android:padding="@dimen/_8sdp"
                android:textColor="?attr/textAppearanceBodySmall"
                android:textColorHint="?attr/textAppearanceBodySmall"
                android:textSize="@dimen/_12sdp"
                app:spinner_arrow_gravity="end"
                app:spinner_arrow_padding="@dimen/_6sdp"
                app:spinner_divider_color="?attr/textAppearanceBodySmall"
                app:spinner_divider_show="false"
                app:spinner_divider_size="0.4dp"
                app:spinner_item_height="@dimen/_35sdp"
                app:spinner_popup_animation="dropdown"
                app:spinner_popup_background="@drawable/round_corners_alert_dialog"
                app:spinner_popup_elevation="@dimen/_11sdp" />
        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/textView_spinnerSpendigsAccountsExplain_FragAlertDialogLongPressMainViewObjects"
            android:layout_below="@+id/cardView_spinnerWidgetStyleHolder_FragAlertDialogLongPressMainViewObjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_marginTop="@dimen/_11sdp"
            android:text="@string/alertDialog_LongPressMainViewObjects_TextView_SpendingsAccounts_Text"
            android:textAlignment="center"
            android:textColor="?attr/textAppearanceBodySmall"
            android:textSize="@dimen/_18sdp" />
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_spinnerSpendigsAccountsHolder_FragAlertDialogLongPressMainViewObjects"
            android:layout_below="@+id/textView_spinnerSpendigsAccountsExplain_FragAlertDialogLongPressMainViewObjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorPrimary"
            app:cardCornerRadius="@dimen/_23sdp">
            <com.skydoves.powerspinner.PowerSpinnerView
                android:id="@+id/spinner_spendigsAccounts_FragAlertDialogLongPressMainViewObjects"
                android:hint="@string/alertDialog_LongPressMainViewObjects_spinner_SpendingsAccounts_StarterText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_16sdp"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_gravity="center"
                android:background="?attr/colorPrimary"
                android:gravity="center"
                android:padding="@dimen/_8sdp"
                android:textColor="?attr/textAppearanceBodySmall"
                android:textColorHint="?attr/textAppearanceBodySmall"
                android:textSize="@dimen/_12sdp"
                app:spinner_arrow_gravity="end"
                app:spinner_arrow_padding="@dimen/_6sdp"
                app:spinner_divider_color="?attr/textAppearanceBodySmall"
                app:spinner_divider_show="false"
                app:spinner_divider_size="0.4dp"
                app:spinner_item_height="@dimen/_35sdp"
                app:spinner_popup_animation="dropdown"
                app:spinner_popup_background="@drawable/round_corners_alert_dialog"
                app:spinner_popup_elevation="@dimen/_11sdp" />
        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/textView_spinnerSpendigsSubAccountsExplain_FragAlertDialogLongPressMainViewObjects"
            android:layout_below="@+id/cardView_spinnerSpendigsAccountsHolder_FragAlertDialogLongPressMainViewObjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|start"
            android:layout_marginTop="@dimen/_11sdp"
            android:text="@string/alertDialog_LongPressMainViewObjects_TextView_SpendingsSubAccounts_Text"
            android:textAlignment="center"
            android:textColor="?attr/textAppearanceBodySmall"
            android:textSize="@dimen/_18sdp" />
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_spinnerSpendigsSubAccountsHolder_FragAlertDialogLongPressMainViewObjects"
            android:layout_below="@+id/textView_spinnerSpendigsSubAccountsExplain_FragAlertDialogLongPressMainViewObjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_marginRight="@dimen/_8sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            app:cardBackgroundColor="?attr/colorPrimary"
            app:cardCornerRadius="@dimen/_23sdp">
            <com.skydoves.powerspinner.PowerSpinnerView
                android:id="@+id/spinner_spendigsSubAccounts_FragAlertDialogLongPressMainViewObjects"
                android:hint="@string/alertDialog_LongPressMainViewObjects_spinner_SpendingsSubAccounts_StarterText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/_16sdp"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginBottom="@dimen/_2sdp"
                android:layout_gravity="center"
                android:background="?attr/colorPrimary"
                android:gravity="center"
                android:padding="@dimen/_8sdp"
                android:textColor="?attr/textAppearanceBodySmall"
                android:textColorHint="?attr/textAppearanceBodySmall"
                android:textSize="@dimen/_12sdp"
                app:spinner_arrow_gravity="end"
                app:spinner_arrow_padding="@dimen/_6sdp"
                app:spinner_divider_color="?attr/textAppearanceBodySmall"
                app:spinner_divider_show="false"
                app:spinner_divider_size="0.4dp"
                app:spinner_item_height="@dimen/_35sdp"
                app:spinner_popup_animation="dropdown"
                app:spinner_popup_background="@drawable/round_corners_alert_dialog"
                app:spinner_popup_elevation="@dimen/_11sdp" />
        </androidx.cardview.widget.CardView>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11sdp"
            android:layout_below="@+id/cardView_spinnerSpendigsSubAccountsHolder_FragAlertDialogLongPressMainViewObjects">
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_Cancel_FragAlertDialogLongPressMainViewObjects"
                android:layout_gravity="left"
                android:textSize="@dimen/_12sdp"
                android:paddingTop="@dimen/_11sdp"
                android:paddingBottom="@dimen/_11sdp"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingRight="@dimen/_15sdp"
                android:layout_marginRight="@dimen/_8sdp"
                android:text="@string/general_cancel"
                android:textColor="?attr/textAppearanceBodySmall"
                android:background="@drawable/round_button_principal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_Confirm_FragAlertDialogLongPressMainViewObjects"
                android:layout_gravity="right"
                android:textSize="@dimen/_12sdp"
                android:paddingTop="@dimen/_11sdp"
                android:paddingBottom="@dimen/_11sdp"
                android:paddingLeft="@dimen/_15sdp"
                android:paddingRight="@dimen/_15sdp"
                android:layout_marginLeft="@dimen/_8sdp"
                android:text="@string/general_confirm"
                android:textColor="?attr/textAppearanceBodySmall"
                android:background="@drawable/round_button_principal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </FrameLayout>
    </RelativeLayout>
</FrameLayout>