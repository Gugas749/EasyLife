<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/colorPrimary"
    android:id="@+id/frameLayout_parent_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
    tools:context=".fragments.mainactivityfragments.overview_view.details.howto.MainACOverviewViewSpendingsAccountDetailsHowToEditTitleFragment">

    <!--    -->
    <TextView
        android:id="@+id/textView_next_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_margin="@dimen/_16sdp"
        android:textSize="@dimen/_16sdp"
        android:textStyle="bold"
        android:elevation="@dimen/_3sdp"
        android:textColor="?attr/textAppearanceBodyMedium"
        android:text="@string/general_next"/>
    <TextView
        android:id="@+id/textView_previous_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|left"
        android:layout_margin="@dimen/_16sdp"
        android:textSize="@dimen/_16sdp"
        android:textStyle="bold"
        android:textColor="?attr/textAppearanceBodyMedium"
        android:text="@string/general_previous"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_16sdp"
        android:layout_gravity="bottom|center">
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_FragIndicator_1_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_4sdp"
            app:cardCornerRadius="@dimen/_90sdp"
            app:cardBackgroundColor="?attr/colorSecondary"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_FragIndicator_2_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_4sdp"
            app:cardCornerRadius="@dimen/_90sdp"
            app:cardBackgroundColor="?attr/colorControlNormal"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_FragIndicator_3_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_4sdp"
            app:cardCornerRadius="@dimen/_90sdp"
            app:cardBackgroundColor="?attr/colorSecondary"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_FragIndicator_4_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_4sdp"
            app:cardCornerRadius="@dimen/_90sdp"
            app:cardBackgroundColor="?attr/colorSecondary"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_FragIndicator_5_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="@dimen/_5sdp"
            android:layout_height="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_4sdp"
            app:cardCornerRadius="@dimen/_90sdp"
            app:cardBackgroundColor="?attr/colorSecondary"/>
    </LinearLayout>
    <!--    -->

    <TextView
        android:layout_gravity="center_horizontal"
        android:textSize="@dimen/_23sdp"
        android:layout_marginTop="@dimen/_23sdp"
        android:text="@string/mainAc_FragSpendingsViewSpendingAccountDetails_HowTo_EditTitle_Title"
        android:textColor="?attr/textAppearanceBodyMedium"
        android:fontFamily="@font/coiny_regular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingBottom="@dimen/_16sdp">
        <TextView
            android:id="@+id/textView_mainTextView_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:textSize="@dimen/_23sdp"
            android:paddingRight="@dimen/_8sdp"
            android:paddingLeft="@dimen/_8sdp"
            android:layout_marginBottom="@dimen/_8sdp"
            android:text="@string/mainAc_FragSpendingsViewSpendingAccountDetails_HowTo_EditTitle_Text"
            android:textColor="?attr/textAppearanceBodyMedium"
            android:textAlignment="center"
            android:fontFamily="@font/dosis_regular"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <FrameLayout
            android:id="@+id/frameLayout_containerAnim_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView_mainTextView_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_marginTop="@dimen/_16sdp">
            <androidx.cardview.widget.CardView
                android:id="@+id/cardView_editText_AccountName_holder_FragMainACOverviewViewSpendingAccountDetailsForm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_16sdp"
                android:layout_marginRight="@dimen/_16sdp"
                android:visibility="invisible"
                app:cardCornerRadius="@dimen/_23sdp"
                app:cardBackgroundColor="?attr/colorSecondary">
                <EditText
                    android:id="@+id/editText_AccountName_FragMainACOverviewViewSpendingAccountDetailsForm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_4sdp"
                    android:text="@string/general_Example"
                    android:alpha="0.6"
                    android:textStyle="bold"
                    android:textColor="?attr/textAppearanceBodyMedium"
                    android:textAlignment="center"
                    android:textSize="@dimen/_16sdp"
                    android:inputType="text"
                    android:background="@android:color/transparent" />
            </androidx.cardview.widget.CardView>
        </FrameLayout>
        <ImageView
            android:id="@+id/imageView_pointingToImageView_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_below="@+id/frameLayout_containerAnim_FragMainACOverviewViewSpendingsAccountDetailsHowToEditTitle"
            android:layout_width="@dimen/_54sdp"
            android:layout_height="@dimen/_54sdp"
            android:elevation="@dimen/_8sdp"
            android:visibility="invisible"
            android:layout_marginLeft="@dimen/_135sdp"
            android:src="@drawable/hand_pointing"
            app:tint="?attr/colorControlNormal" />
    </RelativeLayout>
</FrameLayout>