                                android:text="Percentage 1"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage7_FragMainACOverviewViewSpendingAccountDetailsForm"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
                                app:cardCornerRadius="30sp"
                                android:layout_margin="5sp"/>
                            <TextView
                                android:id="@+id/textView_PercentageText7_2_FragMainACOverviewViewSpendingAccountDetailsForm"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
