                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Name 3"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linerLayout_pieChart_Percentage4_FragMainACOverviewViewSpendingAccountDetailsForm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/linerLayout_pieChart_Percentage3_FragMainACOverviewViewSpendingAccountDetailsForm">
                            <TextView
                                android:id="@+id/textView_PercentageText4_1_FragMainACOverviewViewSpendingAccountDetailsForm"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Percentage 4"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage4_FragMainACOverviewViewSpendingAccountDetailsForm"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
