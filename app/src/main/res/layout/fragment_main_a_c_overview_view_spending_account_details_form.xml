                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linerLayout_pieChart_Percentage7_FragMainACOverviewViewSpendingAccountDetailsForm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/linerLayout_pieChart_Percentage6_FragMainACOverviewViewSpendingAccountDetailsForm">
                            <TextView
                                android:id="@+id/textView_PercentageText7_1_FragMainACOverviewViewSpendingAccountDetailsForm"
