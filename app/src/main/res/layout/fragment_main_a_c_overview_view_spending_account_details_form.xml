                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/linerLayout_pieChart_Percentage2_FragMainACOverviewViewSpendingAccountDetailsForm">
                            <TextView
                                android:id="@+id/textView_PercentageText3_1_FragMainACOverviewViewSpendingAccountDetailsForm"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Percentage 3"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage3_FragMainACOverviewViewSpendingAccountDetailsForm"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
                                app:cardCornerRadius="30sp"
