<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/colorPrimary"
    tools:context=".fragments.mainactivityfragments.overview_view.details.MainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetailsFragment">

    <RelativeLayout
        android:id="@+id/relativeLayout_parent_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!-- TOP NAVIGATION -->
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_topNavigation_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-20sp"
            app:cardCornerRadius="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView_buttonExit_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                android:layout_width="45sp"
                android:layout_height="45sp"
                android:layout_gravity="start"
                android:layout_marginTop="@dimen/_28sdp"
                android:layout_marginLeft="20sp"
                android:layout_marginBottom="@dimen/_14sdp"
                android:padding="10sp"
                android:rotation="180"
                android:src="@drawable/arrow_icon" />
            <ImageView
                android:id="@+id/imageView_buttonHowToUse_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                android:layout_width="45sp"
                android:layout_height="45sp"
                android:layout_marginTop="10sp"
                android:layout_gravity="center"
                android:padding="7sp"
                android:visibility="gone"
                android:src="@drawable/questionpoint_icon" />
            <ImageView
                android:id="@+id/imageView_buttonActivateEdition_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                android:layout_width="45sp"
                android:layout_height="45sp"
                android:layout_gravity="end"
                android:layout_marginTop="@dimen/_28sdp"
                android:layout_marginRight="20sp"
                android:layout_marginBottom="@dimen/_14sdp"
                android:padding="7sp"
                android:src="@drawable/edit_icon" />
        </androidx.cardview.widget.CardView>
        <!--                -->
        <ScrollView
            android:layout_below="@+id/cardView_topNavigation_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_editText_AccountName_holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    app:cardCornerRadius="30sp"
                    app:cardBackgroundColor="?attr/colorSecondary">
                    <EditText
                        android:id="@+id/editText_AccountName_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5sp"
                        android:text="@string/mainAc_FragOverviewViewAddSpendingsAccount_textView_editTextHelper_1_Text"
                        android:textStyle="bold"
                        android:textColor="?attr/textAppearanceBodyMedium"
                        android:textAlignment="center"
                        android:textSize="20sp"
                        android:inputType="text"
                        android:background="@android:color/transparent" />
                </androidx.cardview.widget.CardView>
                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/pieChart_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/cardView_editText_AccountName_holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="250sp"
                    android:layout_marginBottom="10sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    android:layout_marginTop="10sp" />
                <LinearLayout
                    android:id="@+id/linearLayout_percentagesColorNamesValuesHolder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@id/pieChart_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10sp">
                    <RelativeLayout
                        android:layout_width="0sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center">
                        <LinearLayout
                            android:id="@+id/linerLayout_pieChart_Percentage1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/textView_PercentageText1_1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Percentage 1"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
                                app:cardCornerRadius="30sp"
                                android:layout_margin="5sp"/>
                            <TextView
                                android:id="@+id/textView_PercentageText1_2_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Name 1"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linerLayout_pieChart_Percentage2_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/linerLayout_pieChart_Percentage1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails">
                            <TextView
                                android:id="@+id/textView_PercentageText2_1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Percentage 2"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage2_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
                                app:cardCornerRadius="30sp"
                                android:layout_margin="5sp"/>
                            <TextView
                                android:id="@+id/textView_PercentageText2_2_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Name 2"/>
                        </LinearLayout>
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center">
                        <LinearLayout
                            android:id="@+id/linerLayout_pieChart_Percentage3_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/textView_PercentageText3_1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Percentage 3"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage3_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
                                app:cardCornerRadius="30sp"
                                android:layout_margin="5sp"/>
                            <TextView
                                android:id="@+id/textView_PercentageText3_2_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Name 3"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linerLayout_pieChart_Percentage4_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/linerLayout_pieChart_Percentage3_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails">
                            <TextView
                                android:id="@+id/textView_PercentageText4_1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Percentage 1"/>
                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView_colorIndicator_Percentage4_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="10sp"
                                android:layout_height="10sp"
                                app:cardCornerRadius="30sp"
                                android:layout_margin="5sp"/>
                            <TextView
                                android:id="@+id/textView_PercentageText4_2_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="?attr/textAppearanceBodyMedium"
                                android:text="Name 1"/>
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>
                <View
                    android:id="@+id/view_separator_1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/linearLayout_percentagesColorNamesValuesHolder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_15sdp"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:background="?attr/colorControlNormal" />
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_textView_subAccount_1_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/view_separator_1_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    app:cardCornerRadius="30sp"
                    app:cardBackgroundColor="?attr/colorSecondary">
                    <TextView
                        android:id="@+id/textView_subAccount_1_Title_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="subaccount"
                        android:padding="3sp"
                        android:textStyle="bold"
                        android:textColor="?attr/textAppearanceBodyMedium"
                        android:textAlignment="center"
                        android:textSize="@dimen/_14sdp"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_textView_subAccount_2_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/cardView_textView_subAccount_1_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    app:cardCornerRadius="30sp"
                    app:cardBackgroundColor="?attr/colorSecondary">
                    <TextView
                        android:id="@+id/textView_subAccount_2_Title_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="subaccount"
                        android:padding="3sp"
                        android:textStyle="bold"
                        android:textColor="?attr/textAppearanceBodyMedium"
                        android:textAlignment="center"
                        android:textSize="@dimen/_14sdp"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_textView_subAccount_3_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/cardView_textView_subAccount_2_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    app:cardCornerRadius="30sp"
                    app:cardBackgroundColor="?attr/colorSecondary">
                    <TextView
                        android:id="@+id/textView_subAccount_3_Title_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="subaccount"
                        android:padding="3sp"
                        android:textStyle="bold"
                        android:textColor="?attr/textAppearanceBodyMedium"
                        android:textAlignment="center"
                        android:textSize="@dimen/_14sdp"/>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView_textView_subAccount_4_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/cardView_textView_subAccount_3_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    app:cardCornerRadius="30sp"
                    app:cardBackgroundColor="?attr/colorSecondary">
                    <TextView
                        android:id="@+id/textView_subAccount_4_Title_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="subaccount"
                        android:padding="3sp"
                        android:textStyle="bold"
                        android:textColor="?attr/textAppearanceBodyMedium"
                        android:textAlignment="center"
                        android:textSize="@dimen/_14sdp"/>
                </androidx.cardview.widget.CardView>
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/button_deleteSubAccount_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_below="@+id/cardView_textView_subAccount_4_Title_Holder_FragMainACOverviewViewSpendingAccountDetailsFormSubSpendingAccountDetails"
                    android:layout_marginTop="15sp"
                    android:layout_marginLeft="20sp"
                    android:layout_marginRight="20sp"
                    android:layout_marginBottom="15sp"
                    android:textSize="16sp"
                    android:text="@string/mainAc_FragOverviewViewSpendingAccountDetailsForm_Button_DeleteSubAccount_Text"
                    android:textColor="?attr/textAppearanceBodySmall"
                    android:background="@drawable/round_button_deleteandremove"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>
</FrameLayout>